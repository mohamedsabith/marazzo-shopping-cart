<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script type="text/javascript">
function addToCart(proId){
    $.ajax({
        url:'/cart/'+proId,
        method:'get',
        success:(response)=>{
           if(response.error){
              swal("A product already exists in cart.", {
                    icon: "error",
                    })
           }else if(response.msge){
           swal("Cart successfully updated.", {
                    icon: "success",
                    })
            $('#cart-count').html(response.count)
           }else{
             window.location = "/login"
           }
        }
    })
}

function addWishlist(proId){
   $.ajax({
        url:'/wishlistAdd/'+proId,
        method:'get',
        success:(response)=>{
          if(response.errWishlist){
            swal("Product already added.", {
            icon: "warning",
            })
          }else if(response.msge){
           swal("Added to your Wishlist", {
            icon: "success",
            })
          }else{
            window.location = "/login"
          }
        }
    })
}
</script>